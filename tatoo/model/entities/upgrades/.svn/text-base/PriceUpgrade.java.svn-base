package tatoo.model.entities.upgrades;


public class PriceUpgrade extends Upgrade {

	public PriceUpgrade(String name, int price)
	{
		this.name = name;
		this.price = price;
	}
	@Override
	public String getName() {
		return name;
	}

	@Override
	public int getPrice() {
		return price;
	}
	
	@Override
	public String toString() {
		return new String(
				receivingEntity.toString() + "," + name + "->" + price);
	}


	

}
